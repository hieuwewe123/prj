<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ControllerCustomer</servlet-name>
        <servlet-class>controller.ControllerCustomer</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ControllerProduct</servlet-name>
        <servlet-class>controller.ControllerProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerOrder</servlet-name>
        <servlet-class>controller.ControllerOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerOrderItems</servlet-name>
        <servlet-class>controller.ControllerOrderItems</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerStocks</servlet-name>
        <servlet-class>ControllerStocks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllersStocks</servlet-name>
        <servlet-class>controller.ControllersStocks</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerStores</servlet-name>
        <servlet-class>controller.ControllerStores</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerCustomer</servlet-name>
        <url-pattern>/URLCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerProduct</servlet-name>
        <url-pattern>/URLProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerOrder</servlet-name>
        <url-pattern>/URLOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerOrderItems</servlet-name>
        <url-pattern>/URLOrderItems</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllersStocks</servlet-name>
        <url-pattern>/URLStocks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerStores</servlet-name>
        <url-pattern>/URLStores</url-pattern>
    </servlet-mapping>
</web-app>
